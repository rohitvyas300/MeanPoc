doctype html
head
  title Index Page
  meta(charset='utf-8')
  meta(name='viewport' content='width=device-width, initial-scale=1')
.container
  form#formmonth(name='month' method='post' action='/create/')
    .row
      h4 Employee Details
      .input-group.input-group-icon
        input(type='text' placeholder='Emp Number' name='EmpNum')
        input(type='text' placeholder='Emp Name' name='EmpName')
        .input-icon
          i.fa.fa-user1
      .input-group.input-group-icon
        input(type='text' placeholder='Role' name='Role')
        input(type='text' placeholder='Emp Role Code' name='EmpRoleCode')
        .input-icon
          i.fa.fa-envelope
      .input-group.input-group-icon
        input(type='text' placeholder='Act Desc' name='ActDesc')
        input(type='text' placeholder='Emp Band Code' name='EmpBandCode')
        .input-icon
          i.fa.fa-key1
      .input-group.input-group-icon
        input(type='text' placeholder='Location' name='Location')
        input(type='text' placeholder='City' name='City')
        .input-icon
          i.fa.fa-key2
      .input-group.input-group-icon
        input(type='text' placeholder='Project Code' name='ProjectCode')
        .input-icon
          i.fa.fa-key3
    .row
      h4 Employer Details
      .input-group
        .col-third
        .col-third
          input(type='text' placeholder='Source' name='Source')
          input(type='text' placeholder='Assign Account' name='AssignAccount')
        .col-third
          input(type='text' placeholder='JlEmail' name='JlEmail')
          input(type='text' placeholder='Master Customer' name='MasterCustomer')
        .col-third
          input(type='text' placeholder='Portfolio' name='Portfolio')
          input(type='text' placeholder='DU' name='DU')
        .col-third
          input(type='text' placeholder='AccountSpoke' name='AccountSpoke')
          input(type='text' placeholder='site' name='site')
    .row
