            input(type='text' placeholder='Sdet Foundation' name='SdetFoundation')
            input(type='text' placeholder='certis Completed' name='certisCompleted')
          .col-third
            input.textbox-n(placeholder='Fssf plan Date' name='FssFplanDate' type='text' onfocus="(this.type='date')")
            input.textbox-n(placeholder='Fssf ActualDate' name='FssfActualDate' type='text' onfocus="(this.type='date')")
          input(type='text' placeholder='Sdettag' name='Sdettag')
        h5 Agile
        .col-third
          input(placeholder='Agile Plan Date' name='AgilePlanDate' type='text' onfocus="(this.type='date')")
